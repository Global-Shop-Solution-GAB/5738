Program.Sub.cmdAdd_Click.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare

	V.Local.sRet.Declare
	V.Local.iCnt.Declare
	V.Local.bDup.Declare

	V.Local.bDup.Set(False)
	F.Intrinsic.UI.SetBrowserHotTypeAhead(True)

	F.Intrinsic.Control.SelectCase(V.Screen.frmLot!tabLot.Tab)
		F.Intrinsic.Control.Case(0)
			F.Intrinsic.UI.Browser(205,"",V.Local.sRet)
			F.Intrinsic.Control.If(V.Local.sRet,<>,"***CANCEL***")
				F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
				F.Intrinsic.Control.For(V.Local.iCnt,0,V.DataTable.dtCust.RowCount--,1)
					F.Intrinsic.Control.If(V.DataTable.dtCust(V.Local.iCnt).Cust!FieldValTrim,=,V.Local.sRet(0).Trim)
						V.Local.bDup.Set(True)
					F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.Next(V.Local.iCnt)
				F.Intrinsic.Control.If(V.Local.bDup,=,False)
					F.Data.DataTable.AddRow("dtCust","Cust",V.Local.sRet(0).Trim,"Name",V.Local.sRet(1).Trim)
					F.Data.DataTable.SaveToDB("dtCust","con","Gab_5738_Lot_Cust","Cust*!*Name",7,"Cust@!@Cust*!*Name@!@Name*!*Path@!@Path")
					F.Intrinsic.Control.CallSub(loaddtcust)
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.Case(1)
			F.Intrinsic.UI.Browser("Select a Part","con","Select Part, Location, Description From V_Inventory_Mstr;","Part*!*Loc*!*Description","300*!*50*!*700",V.Local.sRet)
			F.Intrinsic.Control.If(V.Local.sRet,<>,"***CANCEL***")
				F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
				F.Intrinsic.Control.For(V.Local.iCnt,0,V.DataTable.dtPart.RowCount--,1)
					F.Intrinsic.Control.If(V.DataTable.dtPart(V.Local.iCnt).Part!FieldValTrim,=,V.Local.sRet(0).Trim)
						V.Local.bDup.Set(True)
					F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.Next(V.Local.iCnt)
				F.Intrinsic.Control.If(V.Local.bDup,=,False)
					F.Data.DataTable.AddRow("dtPart","Part",V.Local.sRet(0).Trim)
					F.Data.DataTable.SaveToDB("dtPart","con","Gab_5738_Lot_Part","Part",7,"Part@!@Part")
					F.Intrinsic.Control.CallSub(loaddtpart)
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndSelect

	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: GAB_5738_Mat_Cert_Setup.g2u{0}{0}Subroutine: {1}{0}Error Occurred {2} with description {3}.",V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.cmdAdd_Click.End

Program.Sub.cmdSavePart_Click.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	V.Local.sSQL.Declare
	V.Local.sTxtPath.Declare
	V.Local.bPathExists.Declare

	F.Intrinsic.Control.SelectCase(V.Screen.frmLot!tabLot.Tab)
		F.Intrinsic.Control.Case(0)
			V.Local.sTxtPath.Set(V.Screen.frmLot!txtPathC.Text)		
			F.Intrinsic.Control.If(V.Local.sTxtPath.Trim,<>,"")
				F.Intrinsic.File.PathExists(v.Local.sTxtPath,V.Local.bPathExists)
				F.Intrinsic.Control.If(V.Local.bPathExists,<>,False)
					F.Intrinsic.String.Build("Update Gab_5738_Lot_Cust Set Path = '{0}' Where Cust = '000000';",V.Local.sTxtPath,V.Local.sSQL)
					F.ODBC.Connection!con.Execute(V.Local.sSQL)
				F.Intrinsic.Control.ElseIf(V.Local.bPathExists,<>,True)
					F.Intrinsic.UI.Msgbox("Please enter a valid path")
					F.Intrinsic.Control.ExitSub
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf
			F.Data.DataTable.SaveToDB("dtCust","con","Gab_5738_Lot_Cust","Cust*!*Name",7,"Cust@!@Cust*!*Name@!@Name*!*Path@!@Path")
			F.Intrinsic.Control.CallSub(loaddtcust)
		F.Intrinsic.Control.Case(1)
			V.Local.sTxtPath.Set(V.Screen.frmLot!txtPath.Text)		
			F.Intrinsic.Control.If(V.Local.sTxtPath.Trim,<>,"")
				F.Intrinsic.File.PathExists(v.Local.sTxtPath,V.Local.bPathExists)
				F.Intrinsic.Control.If(V.Local.bPathExists,<>,False)
					F.Intrinsic.String.Build("Update Gab_5738_Lot_Part Set Path = '{0}' Where Part = '000000';",V.Local.sTxtPath,V.Local.sSQL)
					F.ODBC.Connection!con.Execute(V.Local.sSQL)
				F.Intrinsic.Control.ElseIf(V.Local.bPathExists,<>,True)
					F.Intrinsic.UI.Msgbox("Please enter a valid path")
					F.Intrinsic.Control.ExitSub
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf
			F.Data.DataTable.SaveToDB("dtPart","con","Gab_5738_Lot_Part","Part",7,"Part@!@Part")
			F.Intrinsic.Control.CallSub(loaddtpart)
	F.Intrinsic.Control.EndSelect

	F.Intrinsic.UI.Msgbox("Setup Saved","Save")

	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: GAB_5738_Mat_Cert_Setup.g2u{0}{0}Subroutine: {1}{0}Error Occurred {2} with description {3}.",V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.cmdSavePart_Click.End

