Program.Sub.CheckValues.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare

	V.Local.sSQL.Declare
	V.Local.sOption.Declare
	V.Local.sPartPath.Declare
	V.Local.sPartGroup.Declare

	'Check Option for Customer, Part or Both
	F.ODBC.Connection!con.ExecuteAndReturn("Select Path From Gab_5738_Lot_Part Where Part = '000000OPT';",V.Local.sOption)

	F.Intrinsic.Control.SelectCase(V.Local.sOption.Trim)
		F.Intrinsic.Control.Case("C")
			Gui.frmLot.optCust.Value(True)
		F.Intrinsic.Control.Case("P")
			Gui.frmLot.optPart.Value(True)
	F.Intrinsic.Control.EndSelect

	F.Intrinsic.Control.CallSub(radiobuttons)

	F.Intrinsic.Control.If(V.Local.sOption,<>,"")
		'Check Part default path
		F.ODBC.Connection!con.ExecuteAndReturn("Select Path From Gab_5738_Lot_Part Where Part = '000000';",V.Local.sPartPath)
		Gui.frmLot.txtPath.Text(V.Local.sPartPath.Trim)

		'Check Part default group
		F.ODBC.Connection!con.ExecuteAndReturn("Select Path From Gab_5738_Lot_Part Where Part = '000000GR';",V.Local.sPartGroup)
		Gui.frmLot.txtGrp.Text(V.Local.sPartGroup.Trim)
		F.Intrinsic.Control.If(V.Local.sPartGroup.Trim,<>,"")
			F.Intrinsic.String.Build("Select Grp_Name From Atg_Doc_Grp Where Grp_ID = {0};",V.Local.sPartGroup,V.Local.sSQL)
			F.ODBC.Connection!con.ExecuteAndReturn(V.Local.sSQL,V.Local.sPartGroup)
			Gui.frmLot.txtGrp2.Text(V.Local.sPartGroup.Trim)
		F.Intrinsic.Control.EndIf

		'Check Customer Tab options
		F.ODBC.Connection!con.ExecuteAndReturn("Select Name,Path From Gab_5738_Lot_Cust Where Cust = '000000';",V.Local.sOption)
		F.Intrinsic.String.Split(V.Local.sOption,"*!*",V.Local.sOption)
		F.Intrinsic.Control.SelectCase(V.Local.sOption(0).Trim)
			F.Intrinsic.Control.Case("A")
				Gui.frmLot.optAll.Value(True)
			F.Intrinsic.Control.Case("C")
				Gui.frmLot.optCustom.Value(True)
		F.Intrinsic.Control.EndSelect
		Gui.frmLot.txtPathC.Text(V.Local.sOption(1).Trim)
		F.Intrinsic.Control.CallSub(radiobuttonsC)
		F.Intrinsic.Control.CallSub(tablot_click)
	F.Intrinsic.Control.Else
		F.ODBC.Connection!con.Execute("Insert Into Gab_5738_Lot_Part Values('000000OPT','');")
		F.ODBC.Connection!con.Execute("Insert Into Gab_5738_Lot_Part Values('000000','');")
		F.ODBC.Connection!con.Execute("Insert Into Gab_5738_Lot_Part Values('000000GR','');")
		F.ODBC.Connection!con.Execute("Insert Into Gab_5738_Lot_Cust Values('000000','','');")
		Gui.frmLot.tabLot.Enabled(False)
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: GAB_5738_Mat_Cert_Setup.g2u{0}{0}Subroutine: {1}{0}Error Occurred {2} with description {3}.",V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.CheckValues.End

